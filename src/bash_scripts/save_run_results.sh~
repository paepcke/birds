#cp -r src/birdsong/runs_raw_results/ Experiments/Resnet/Resnet18_Pretrain_0_NoAugsInValSet
#cp -r src/birdsong/runs_models/ Experiments/Resnet/Resnet18_Pretrain_0_NoAugsInValSet
